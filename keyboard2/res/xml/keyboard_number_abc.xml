<?xml version="1.0" encoding="utf-8"?><Keyboard xmlns:android="http://schemas.android.com/apk/res/android"    android:horizontalGap="@dimen/number_horizontal_gap"    android:keyWidth="@dimen/number_key_width"    android:keyHeight="@dimen/number_key_height"    android:verticalGap="@dimen/number_vertical_gap">    <Row android:keyHeight="0%p" />    <Row>        <Key            android:codes="49"            android:keyLabel="1" />        <Key            android:codes="50"            android:keyLabel="2" />        <Key            android:codes="51"            android:keyLabel="3" />        <Key            android:codes="-5"            android:isRepeatable="true"            android:keyHeight="16.5%p"            android:keyIcon="@drawable/ic_outline_backspace_24dp" />        <Key            android:codes="-9999"            android:keyWidth="0%p" />    </Row>    <Row>        <Key            android:codes="52"            android:keyLabel="4" />        <Key            android:codes="53"            android:keyLabel="5" />        <Key            android:codes="54"            android:keyLabel="6" />    </Row>    <Row>        <Key            android:codes="55"            android:keyLabel="7" />        <Key            android:codes="56"            android:keyLabel="8" />        <Key            android:codes="57"            android:keyLabel="9" />        <Key            android:codes="-9998"            android:keyIcon="@drawable/ic_baseline_keyboard_tab_24dp" />    </Row>    <Row>        <Key            android:codes="789789"            android:keyLabel="ABC"            android:keyWidth="@dimen/number_key_width_abc" />        <Key            android:codes="45"            android:keyLabel="-"            android:keyWidth="@dimen/number_key_width_abc" />        <Key            android:codes="48"            android:keyLabel="0" />        <Key            android:codes="46"            android:keyLabel="." />        <Key            android:codes="-4"            android:keyIcon="@drawable/ic_done_24dp" />    </Row>    <Row android:keyHeight="0%p" /></Keyboard>